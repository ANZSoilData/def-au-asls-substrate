subst:alteration
  a skos:Collection ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/alteration"^^xsd:anyURI ;
  skos:definition "Substrate materials may be so extensively altered (as in deep weathering profiles) that it may be difficult or impossible to determine their original nature. Certain constituents may be either depleted or enriched. Thus, in many laterite profiles, some horizons are ferruginised, partially ferruginised and partially kaolinised, and the pallid zone kaolinised. Silicification may also be associated with deep weathering profiles although not exclusively so; for instance, some limestones may be variably silicified. In contrast, calcification, which is widespread in parts of southern Australia, is not usually associated with deep weathering." ;
  skos:member subst:alteration-F ;
  skos:member subst:alteration-K ;
  skos:member subst:alteration-L ;
  skos:member subst:alteration-O ;
  skos:member subst:alteration-S ;
  skos:note "In some instances more than one type of alteration may be present, for example the mottled zone of a laterite profile may be both ferruginised and kaolinised. Where both types of alteration occur, record both." ;
  skos:prefLabel "Alteration - enumeration" ;
  skos:related <https://anzsoil.org/def/au/property/alteration> ;
.
subst:alteration-F
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/alteration-F"^^xsd:anyURI ;
  skos:definition "iron enriched." ;
  skos:notation "F" ;
  skos:prefLabel "Ferruginised" ;
.
subst:alteration-K
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/alteration-K"^^xsd:anyURI ;
  skos:definition "calcium carbonate enriched." ;
  skos:notation "K" ;
  skos:prefLabel "Calcified" ;
.
subst:alteration-L
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/alteration-L"^^xsd:anyURI ;
  skos:definition "clay enriched, usually pale coloured, for example the pallid zone of a laterite profile." ;
  skos:notation "L" ;
  skos:prefLabel "Kaolinised" ;
.
subst:alteration-O
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/alteration-O"^^xsd:anyURI ;
  skos:definition "deeply weathered but no specific nature." ;
  skos:notation "O" ;
  skos:prefLabel "Other" ;
.
subst:alteration-S
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/alteration-S"^^xsd:anyURI ;
  skos:definition "silica enriched." ;
  skos:notation "S" ;
  skos:prefLabel "Silicified" ;
.
subst:confidence
  a skos:Collection ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/confidence"^^xsd:anyURI ;
  skos:definition "The observer should state the degree of confidence that the observed substrate material is in fact the parent material of the observed soil profile or the major part of that profile, that is, of the B horizon." ;
  skos:member subst:confidence-certain ;
  skos:member subst:confidence-doubtful ;
  skos:member subst:confidence-not-parent ;
  skos:member subst:confidence-probable ;
  skos:prefLabel "Confidence that substrate is parent material" ;
  skos:related <https://anzsoil.org/def/au/property/confidence> ;
.
subst:confidence-certain
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/confidence-certain"^^xsd:anyURI ;
  skos:notation "A" ;
  skos:prefLabel "Almost certain or certain" ;
.
subst:confidence-doubtful
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/confidence-doubtful"^^xsd:anyURI ;
  skos:notation "D" ;
  skos:prefLabel "Dubious, doubtful" ;
.
subst:confidence-not-parent
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/confidence-not-parent"^^xsd:anyURI ;
  skos:notation "N" ;
  skos:prefLabel "Not parent material" ;
.
subst:confidence-probable
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/confidence-probable"^^xsd:anyURI ;
  skos:notation "P" ;
  skos:prefLabel "Probable" ;
.
subst:discontinuity-spacing
  a skos:Collection ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/discontinuity-spacing"^^xsd:anyURI ;
  skos:definition "Physical weathering opens up fissures or joints that reduce the strength of the rock mass relative to that of the intact rock material.  These fissures generally increase in number towards the land surface.  The following categories of discontinuity spacing apply (Deere 1968, Selby 1982):" ;
  skos:member subst:discontinuity-spacing-B ;
  skos:member subst:discontinuity-spacing-C ;
  skos:member subst:discontinuity-spacing-F ;
  skos:member subst:discontinuity-spacing-M ;
  skos:member subst:discontinuity-spacing-S ;
  skos:prefLabel "Spacing of discontinuities - enumeration" ;
  skos:related <https://anzsoil.org/def/au/property/discontinuity-spacing> ;
.
subst:discontinuity-spacing-B
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/discontinuity-spacing-B"^^xsd:anyURI ;
  skos:definition "300 mm â€“ 1 m" ;
  skos:notation "B" ;
  skos:prefLabel "blocky; moderately jointed." ;
.
subst:discontinuity-spacing-C
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/discontinuity-spacing-C"^^xsd:anyURI ;
  skos:definition "<50 mm" ;
  skos:notation "C" ;
  skos:prefLabel "crushed or shattered." ;
.
subst:discontinuity-spacing-F
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/discontinuity-spacing-F"^^xsd:anyURI ;
  skos:definition "50-300 mm" ;
  skos:notation "F" ;
  skos:prefLabel "fractured; intensely jointed." ;
.
subst:discontinuity-spacing-M
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/discontinuity-spacing-M"^^xsd:anyURI ;
  skos:definition "1-3 m" ;
  skos:notation "M" ;
  skos:prefLabel "massive; few joints." ;
.
subst:discontinuity-spacing-S
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/discontinuity-spacing-S"^^xsd:anyURI ;
  skos:definition ">3 m" ;
  skos:notation "S" ;
  skos:prefLabel "solid; virtually unjointed" ;
.
subst:genesis-AC
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-AC"^^xsd:anyURI ;
  skos:definition "indurated material rich in aluminium hydroxides. Commonly consists of cemented pisoliths and usually known as bauxite." ;
  skos:notation "AC" ;
  skos:prefLabel "Alcrete (bauxite)" ;
.
subst:genesis-AL
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-AL"^^xsd:anyURI ;
  skos:definition "sediment mass deposited from transport by channelled stream flow or over-bank stream flow." ;
  skos:notation "AL" ;
  skos:prefLabel "Alluvium" ;
.
subst:genesis-AT
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-AT"^^xsd:anyURI ;
  skos:notation "AT" ;
  skos:prefLabel "Other artificially hardened materials" ;
.
subst:genesis-BE
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-BE"^^xsd:anyURI ;
  skos:definition "sediment mass deposited from transport by waves or tides at the shore of a sea or lake." ;
  skos:notation "BE" ;
  skos:prefLabel "Beach sediment" ;
.
subst:genesis-CD
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-CD"^^xsd:anyURI ;
  skos:definition "colluvium slowly displaced a short distance downslope as a result of small irregular movements, with the net movement increasing towards the land surface." ;
  skos:notation "CD" ;
  skos:prefLabel "Creep deposit" ;
.
subst:genesis-CN
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-CN"^^xsd:anyURI ;
  skos:definition "artificial conglomerate rock mass of selected size grade material that has been hardened using Portland cement or other kinds of cement. Concrete is a weak rock (ca. 35 MPa unconfined compressive strength) usually reinforced with steel to increase its tensile strength." ;
  skos:notation "CN" ;
  skos:prefLabel "Concrete" ;
.
subst:genesis-CO
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-CO"^^xsd:anyURI ;
  skos:definition "sediment mass deposited from transport down a slope by gravity (scree), landslide (landslide deposit), mudflow (mudflow deposit), creep (creep deposit) or sheet flow (sheet flow deposit), but not by stream flow. Compared with alluvium, colluvium lacks bedding structure; is more variable in grain size (that is, more poorly sorted); contains much local material; and is generally much more angular. Coarse particles may have particular alignments." ;
  skos:notation "CO" ;
  skos:prefLabel "Colluvium" ;
.
subst:genesis-DR
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-DR"^^xsd:anyURI ;
  skos:definition "weathered material (typically soft and clay-rich) produced by thorough decomposition of rock masses due to exposure to land surface processes, but with no transport. It generally lacks any structures that may have been present in the unweathered rock (see also saprolite and partially weathered rock)." ;
  skos:notation "DR" ;
  skos:prefLabel "Decomposed rock" ;
.
subst:genesis-ED
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-ED"^^xsd:anyURI ;
  skos:definition "sediment mass deposited from transport by the wind." ;
  skos:notation "ED" ;
  skos:prefLabel "Eolian sediment" ;
.
subst:genesis-ES
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-ES"^^xsd:anyURI ;
  skos:definition "eolian sediment of sand size, often taking the form of dunes, with characteristic bedding structures." ;
  skos:notation "ES" ;
  skos:prefLabel "Eolian sand" ;
.
subst:genesis-ET
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-ET"^^xsd:anyURI ;
  skos:definition "consolidated sedimentary rock consisting of clastic material deposited by the wind.* Includes bioclastic calcarenites." ;
  skos:notation "ET" ;
  skos:prefLabel "Eolianite" ;
.
subst:genesis-EV
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-EV"^^xsd:anyURI ;
  skos:definition "weak sedimentary rock or sediment formed by the precipitation of solutes from water bodies on the land surface, typically as lacustrine sediments. Includes halite and gypsum." ;
  skos:notation "EV" ;
  skos:prefLabel "Evaporite" ;
.
subst:genesis-FC
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-FC"^^xsd:anyURI ;
  skos:definition "indurated material rich in hydrated oxides of iron (usually goethite and hematite) occurring as cemented nodules and/or concretions, or as massive sheets. This material has been commonly referred to in local usage around Australia as laterite, duricrust or ironstone." ;
  skos:notation "FC" ;
  skos:prefLabel "Ferricrete" ;
.
subst:genesis-FI
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-FI"^^xsd:anyURI ;
  skos:definition "mass of artificial sediment formed by earth-moving works. Fill is sometimes compacted to the status of a very weak rock mass (stabilised soil), but typically remains an earth mass (Table 36). Garbage forms a very low-density, low-strength fill." ;
  skos:notation "FI" ;
  skos:prefLabel "Fill" ;
.
subst:genesis-GY
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-GY"^^xsd:anyURI ;
  skos:definition "evaporite consisting of hydrated calcium sulphate. Non-hydrated calcium sulphate forms closely related masses called anhydrite. It may subsequently be transported by wind as fine crystals and form lunettes or more widespread sedimentary layers blanketing the landscape." ;
  skos:notation "GY" ;
  skos:prefLabel "Gypsum" ;
.
subst:genesis-HA
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-HA"^^xsd:anyURI ;
  skos:definition "evaporite consisting of sodium chloride." ;
  skos:notation "HA" ;
  skos:prefLabel "Halite (rock salt)" ;
.
subst:genesis-IG
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-IG"^^xsd:anyURI ;
  skos:definition "strong or very strong rock masses formed by solidification of molten rock matter (magma) derived from below the Earthâ€™s surface. The rocks are mainly composed of interlocking crystals. Types are distinguished in Table 34. Plutonic rocks and volcanic rocks are included." ;
  skos:notation "IG" ;
  skos:prefLabel "Igneous rocks" ;
.
subst:genesis-IN
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-IN"^^xsd:anyURI ;
  skos:definition "very weak to strong volcanic rock mass deposited from a flow of ash, the stronger forms being welded together by residual heat during deposition." ;
  skos:notation "IN" ;
  skos:prefLabel "Ignimbrite" ;
.
subst:genesis-KC
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-KC"^^xsd:anyURI ;
  skos:definition "any cemented, terrestrial carbonate accumulation that may vary significantly in morphology and degree of cementation. Also known as carbonate pan, calcareous pan, caliche, kunkar, secondary limestone, travertine. All show slight to strong effervescence with 1-molar HCl." ;
  skos:notation "KC" ;
  skos:prefLabel "Calcrete" ;
.
subst:genesis-LA
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-LA"^^xsd:anyURI ;
  skos:definition "sediment mass deposited from transport by waves and from sediment solution and suspension in still water in a closed depression on land." ;
  skos:notation "LA" ;
  skos:prefLabel "Lacustrine sediment" ;
.
subst:genesis-LC
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-LC"^^xsd:anyURI ;
  skos:definition "strongly indurated siliceous material cemented by, and largely composed of, forms of silica, including quartz, chalcedony, opal and chert." ;
  skos:notation "LC" ;
  skos:prefLabel "Silcrete" ;
.
subst:genesis-LD
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-LD"^^xsd:anyURI ;
  skos:definition "colluvium rapidly displaced many metres downslope by failure of a mass of earth or rock. If the mass is not already a part of the regolith, the landslide incorporates it in the regolith. Original rock structures are fragmented and disorganised by the action of the landslide." ;
  skos:notation "LD" ;
  skos:prefLabel "Landslide deposit" ;
.
subst:genesis-LO
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-LO"^^xsd:anyURI ;
  skos:definition "eolian sediment of silt size." ;
  skos:notation "LO" ;
  skos:prefLabel "Loess" ;
.
subst:genesis-MA
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-MA"^^xsd:anyURI ;
  skos:definition "sediment mass deposited from transport by waves and from solution and suspension in sea water." ;
  skos:notation "MA" ;
  skos:prefLabel "Marine sediment" ;
.
subst:genesis-MD
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-MD"^^xsd:anyURI ;
  skos:definition "colluvium mixed with water to form dense fluid, and rapidly displaced metres or kilometres downslope. The material is more thoroughly disaggregated than that of a landslide deposit and lacks the bedding and sorting of grain sizes seen in alluvium." ;
  skos:notation "MD" ;
  skos:prefLabel "Mudflow deposit" ;
.
subst:genesis-ME
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-ME"^^xsd:anyURI ;
  skos:definition "moderately strong to very strong rock masses formed by the chemical and physical alterations of igneous or sedimentary rocks under high temperatures and/or very high pressures within the Earthâ€™s crust. Types are distinguished in Table 34." ;
  skos:notation "ME" ;
  skos:prefLabel "Metamorphic rocks" ;
.
subst:genesis-PA
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-PA"^^xsd:anyURI ;
  skos:definition "fine-grained calcareous eolian sediment consisting of 30â€“70% clay." ;
  skos:notation "PA" ;
  skos:prefLabel "Parna" ;
.
subst:genesis-PC
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-PC"^^xsd:anyURI ;
  skos:definition "dense argillaceous rock of varying degree of silicification with a conchoidal fracture and general appearance of unglazed porcelain." ;
  skos:notation "PC" ;
  skos:prefLabel "Porcellanite" ;
.
subst:genesis-PL
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-PL"^^xsd:anyURI ;
  skos:definition "igneous rocks solidified at depth within the Earthâ€™s crust." ;
  skos:notation "PL" ;
  skos:prefLabel "Plutonic rocks" ;
.
subst:genesis-PW
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-PW"^^xsd:anyURI ;
  skos:definition "weathered material produced by exposure of rock masses to land surface processes but with no transport. Partial decomposition results in changes in colour, texture, composition, strength or form of the parent rock mass (see also decomposed rock and saprolite)." ;
  skos:notation "PW" ;
  skos:prefLabel "Partially weathered rock" ;
.
subst:genesis-RB
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-RB"^^xsd:anyURI ;
  skos:definition "an informal name used for a particular indurated earthy material (see â€˜Pansâ€™, page 143). In many instances it is not known if the red-brown hardpan is a paleosol or a cemented sediment.*" ;
  skos:notation "RB" ;
  skos:prefLabel "Red-brown hardpan" ;
.
subst:genesis-SA
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-SA"^^xsd:anyURI ;
  skos:definition "a particular form of decomposed rock. It is characterised by the preservation of structures (including â€˜textureâ€™ in the petrological sense) that were present in the unweathered rock." ;
  skos:notation "SA" ;
  skos:prefLabel "Saprolite" ;
.
subst:genesis-SC
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-SC"^^xsd:anyURI ;
  skos:definition "sedimentary rocks in which mineral grains or fragments are not important constituents. The group includes coal, chert and non-fragmental limestones as well as saline rocks (evaporites) such as halite (rock salt) and gypsum. Chemical and organic sedimentary rocks are common in the regolith zone." ;
  skos:notation "SC" ;
  skos:prefLabel "Chemical and organic sedimentary rocks" ;
.
subst:genesis-SD
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-SD"^^xsd:anyURI ;
  skos:definition "sedimentary rocks composed of mineral grains or fragments derived from pre-existing rocks. Types are distinguished in Table 34." ;
  skos:notation "SD" ;
  skos:prefLabel "Detrital sedimentary rocks" ;
.
subst:genesis-SE
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-SE"^^xsd:anyURI ;
  skos:definition "colluvium deposited after falling or rolling from cliffed or steep slopes, consisting of loose rock fragments of gravel size or larger and generally lacking a fine interstitial component." ;
  skos:notation "SE" ;
  skos:prefLabel "Scree" ;
.
subst:genesis-SH
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-SH"^^xsd:anyURI ;
  skos:definition "colluvium deposited from transport by a very shallow flow of water as a sheet, or network of rills on the land surface. Sheet flow deposits are very thin except at the foot of a slope and beneath sheet-flood fans." ;
  skos:notation "SH" ;
  skos:prefLabel "Sheet flow deposit" ;
.
subst:genesis-SP
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-SP"^^xsd:anyURI ;
  skos:definition "sedimentary rocks resulting from the deposition of airborne materials produced by volcanic eruptions." ;
  skos:notation "SP" ;
  skos:prefLabel "Pyroclastic rocks" ;
.
subst:genesis-SR
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-SR"^^xsd:anyURI ;
  skos:definition "weak or moderately strong rock masses formed by the hardening of sediments due to compaction, recrystallisation or cementation. These processes can occur within the regolith but are promoted by burial within the Earthâ€™s crust. Major categories of sedimentary rocks are detrital sedimentary rocks, pyroclastic rocks, and chemical and organic sedimentary rocks." ;
  skos:notation "SR" ;
  skos:prefLabel "Sedimentary rocks" ;
.
subst:genesis-ST
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-ST"^^xsd:anyURI ;
  skos:definition "artificial mass with the strength grade of very weak rock. It results from the â€˜stabilisationâ€™ of an earth mass by a variety of processes: compaction; the admixture of lime, Portland cement, bitumen or other substances; heating; freezing; or electro-hardening. Cement stabilisation can produce a mass as strong as 10 MPa unconfined compressive strength.*" ;
  skos:notation "ST" ;
  skos:prefLabel "Stabilised soil" ;
.
subst:genesis-TI
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-TI"^^xsd:anyURI ;
  skos:definition "sediment mass deposited from transport in ice, as in a glacier. Till is neither bedded nor sorted; it has a matrix of clay or silt enclosing larger particles of unweathered rock ranging up to large boulders." ;
  skos:notation "TI" ;
  skos:prefLabel "Till" ;
.
subst:genesis-VA
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-VA"^^xsd:anyURI ;
  skos:definition "eolian sediment consisting of relatively fine (<2 mm) pyroclastic material. It often contains a proportion of highly weatherable glass." ;
  skos:notation "VA" ;
  skos:prefLabel "Volcanic ash" ;
.
subst:genesis-VO
  a skos:Concept ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/genesis-VO"^^xsd:anyURI ;
  skos:definition "igneous rocks solidified after eruption on to the land surface." ;
  skos:notation "VO" ;
  skos:prefLabel "Volcanic rocks" ;
.
subst:grain-size
  a skos:Collection ;
  dcterms:identifier "http://anzsoil.org/def/au/asls/substrate/grain-size"^^xsd:anyURI ;
  skos:definition "It is informative to estimate the size of the most common particles of a substrate material whether the material is thought to be of sedimentary, metamorphic or igneous origin." ;
  skos:member subst:grain-size-1 ;
  skos:member subst:grain-size-2 ;
  skos:member subst:grain-size-3 ;
  skos:note "* As shown by Figure 15, page 116, these size grade limits agree with the scale of Wentworth (1922), adopted by the United States National Research Council (Pettijohn 1957) and the American Geophysical Union (Lane et al. 1947), and with that of the Massachusetts Institute of Technology, also adopted by the British Standards Institution (1975) and the Standards Association of Australia (1977). The 0.06 mm boundary criterion (strictly 0.063 mm) separating sand and silt differs markedly from the value 0.02 mm of the International Scheme (proposed originally by Atterberg (1905) and accepted by the International Society of Soil Science); the 0.05 mm value of the United States Department of Agriculture (Soil Survey Staff 1975); and the 0.075 mm value of the Unified Soil Classification (Anon. 1953)." ;
