  rdfs:label "'Medium Gravelly'@en" ;
  xsd:maxExclusive "20" ;
  xsd:minInclusive "6" ;
  skos:definition "Medium gravelly, 6-20mm" ;
  skos:notation "2" ;
  skos:prefLabel "'Medium Gravelly'@en" ;
.
cf_size:stony
  rdf:type skos:Concept ;
  dct:description "Stony, 200-600mm" ;
