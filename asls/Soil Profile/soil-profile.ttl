  skos:prefLabel "Reoriented"@en ;
.
sp:coarse-fragments-distribution-S
  a skos:Concept ;
  dct:description "fragments occur in bands, usually parallel with the soil surface (excluding those parallel with joint or bedding planes of the bedrock). They may include materials other than those from the underlying bedrock."@en ;
  dct:identifier "http://anzsoil.org/def/au/asls/soil-profile/coarse-fragments-distribution-S"^^xsd:anyURI ;
  ui:isMemberOf sp:coarse-fragments-distribution ;
  rdfs:comment "C_CF_DISTRIBUTION" ;
  rdfs:label "Stratified"@en ;
