  skos:notation "S" ;
  skos:prefLabel "Stratified"@en ;
.
sp:coarse-fragments-distribution-U
  a skos:Concept ;
  dct:description "all fragments are remnants of the underlying bedrock and their orientation closely parallels that of the joint or bedding planes of the bedrock."@en ;
  dct:identifier "http://anzsoil.org/def/au/asls/soil-profile/coarse-fragments-distribution-U"^^xsd:anyURI ;
